{
  "kind": "collectionType",
  "collectionName": "carts",
  "info": {
    "singularName": "cart",
    "pluralName": "carts",
    "displayName": "Cart",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "location": {
      "type": "json"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "confirmed",
        "processed",
        "received"
      ]
    },
    "total": {
      "type": "decimal"
    },
    "payment_method": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::payment-method.payment-method",
      "inversedBy": "cart"
    },
    "delivery": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::delivery.delivery",
      "inversedBy": "carts"
    },
    "vat": {
      "type": "decimal"
    },
    "orderProductQuantity": {
      "type": "json"
    },
    "reference": {
      "type": "string"
    },
    "universal": {
      "type": "uid"
    },
    "stores": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::store.store",
      "inversedBy": "carts"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "carts"
    }
  }
}
